<td class="ta-c">
    {% if delete is not defined or delete != false %}
        <a class="icon-trash-o c-ko td-n fz-l"
           title="{{ 'ui.forms.delete'|trans }}"
           href="{{ url("admin_#{class}_delete", { id: entity.id }) }}"
           data-tc-modules="confirm" data-tc-name="{{ entity.name }}"
           data-tc-text="{{ 'ui.confirm.delete'|trans }}">
        </a>
    {% endif %}
</td>
