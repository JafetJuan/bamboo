{#
    This template needs this formatted data:

    {
        rows: {
            "id": {width:"25"}
        }
    }

    You can override the row name by defining "row" field
#}

{% for row, info in rows %}
    <th role="columnheader"
        {%- if info.width is defined -%}
            class="w-{{ info.width }}"
        {%- endif -%}
    >
        {{- row|trans -}}
    </th>
{% endfor %}
