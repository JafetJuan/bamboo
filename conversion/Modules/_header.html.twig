<!DOCTYPE HTML>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7"{% if language_code is defined and language_code %} lang="{% language_code|escape:'html':'UTF-8' %}"{% endif %}><![endif]-->
<!--[if IE 7]><html class="no-js lt-ie9 lt-ie8 ie7"{% if language_code is defined and language_code %} lang="{% language_code|escape:'html':'UTF-8' %}"{% endif %}><![endif]-->
<!--[if IE 8]><html class="no-js lt-ie9 ie8"{% if language_code is defined and language_code %} lang="{% language_code|escape:'html':'UTF-8' %}"{% endif %}><![endif]-->
<!--[if gt IE 8]> <html class="no-js ie9"{% if language_code is defined and language_code %} lang="{% language_code|escape:'html':'UTF-8' %}"{% endif %}><![endif]-->
<html{% if language_code is defined and language_code %} lang="{% language_code|escape:'html':'UTF-8' %}"{% endif %} {% if IS_RTL is defined and IS_RTL %} dir="rtl" class="rtl{% if LEO_DEFAULT_SKIN is defined %} {% LEO_DEFAULT_SKIN %}{% endif %}" {% else %} class="{% if LEO_DEFAULT_SKIN is defined %}{% LEO_DEFAULT_SKIN %}{% endif %}" {% endif %}>
	{% include "@~~bundle_name~~/layout/setting.tpl" %}
	<head>
		<meta charset="utf-8" />
		<title>{% meta_title|escape:'html':'UTF-8' %}</title>
		{% if meta_description is defined AND meta_description %}
			<meta name="description" content="{% meta_description|escape:'html':'UTF-8' %}" />
		{% endif %}
		{% if meta_keywords is defined AND meta_keywords %}
			<meta name="keywords" content="{% meta_keywords|escape:'html':'UTF-8' %}" />
		{% endif %}
		<meta name="generator" content="PrestaShop" />
		<meta name="robots" content="{% if nobots is defined %}no{% endif %}index,{% if nofollow is defined and nofollow %}no{% endif %}follow" />
		{% if ENABLE_RESPONSIVE %}<meta name="viewport" content="width=device-width, minimum-scale=0.25, maximum-scale=1.6, initial-scale=1.0" />{% endif %}
		<meta name="apple-mobile-web-app-capable" content="yes" />
		<link rel="icon" type="image/vnd.microsoft.icon" href="{% favicon_url}?{img_update_time %}" />
		<link rel="shortcut icon" type="image/x-icon" href="{% favicon_url}?{img_update_time %}" />
		{% if css_files is defined %}
			{foreach from=css_files key=css_uri item=media}
				<link rel="stylesheet" href="{% css_uri|escape:'html':'UTF-8' %}" type="text/css" media="{% media|escape:'html':'UTF-8' %}" />
			{% /foreach %}
		{% endif %}
		{% if js_defer is defined and not js_defer and js_files is defined and js_def is defined %}
			{% js_def %}
			{foreach from=js_files item=js_uri}
			<script type="text/javascript" src="{% js_uri|escape:'html':'UTF-8' %}"></script>
			{% /foreach %}
{% endif %}
		{% if ENABLE_RESPONSIVE %}
		<link rel="stylesheet" type="text/css" href="{% content_dir}themes/{LEO_THEMENAME %}/css/responsive.css"/>
		{% else %}
		<link rel="stylesheet" type="text/css" href="{% content_dir}themes/{LEO_THEMENAME %}/css/non-responsive.css"/>
		{% endif %}
		<link rel="stylesheet" type="text/css" href="{% content_dir}themes/{LEO_THEMENAME %}/css/font-awesome.min.css"/>
		{% HOOK_HEADER %}
                {% if LEO_CSS is defined %}{foreach from=LEO_CSS key=css_uri item=media}
                <link rel="stylesheet" href="{% css_uri %}" type="text/css" media="{% media %}" />
                {% /foreach %}{% endif %}
		{% if CUSTOM_FONT is defined %}
                {% CUSTOM_FONT %}
		{% endif %}
		{% if LAYOUT_WIDTH is defined %}
                {% LAYOUT_WIDTH %}
		{% endif %}
		<link rel="stylesheet" href="http{% if Tools::usingSecureMode() %}s{% endif %}://fonts.googleapis.com/css?family=Open+Sans:300,600&amp;subset=latin,latin-ext" type="text/css" media="all" />
		<link href='http://fonts.googleapis.com/css?family=Roboto+Condensed' rel='stylesheet' type='text/css'>
		<!--[if IE 8]>
		<script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
		<script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
		<![endif]-->
		{% if LEO_SKIN_CSS is defined %}
			{foreach from=LEO_SKIN_CSS item=linkCss}
				{% linkCss %}
			{% /foreach %}
		{% endif %}
	</head>
	<body{% if page_name is defined %} id="{% page_name|escape:'html':'UTF-8' %}"{% endif %} class="{% if page_name is defined %}{% page_name|escape:'html':'UTF-8' %}{% endif %}{% if body_classes is defined and body_classes|@count %} {implode value=body_classes separator=' '}{% endif %}{% if hide_left_column %} hide-left-column{% else %} show-left-column{% endif %}{% if hide_right_column %} hide-right-column{% else %} show-right-column{% endif %}{% if content_only is defined and content_only %} content_only{% endif %} lang_{% lang_iso %}  {% if LEO_LAYOUT_MODE is defined %}{% LEO_LAYOUT_MODE %}{% endif %}{% if USE_FHEADER is defined and USE_FHEADER %} keep-header{% endif %}{% if LEO_HEADER_STYLE is defined %} {% LEO_HEADER_STYLE %}{% endif %}{% if LEO_HEADER_STYLE not ="header-hide-topmenu" and LEO_SIDEBAR_MENU not ="sidebar-hide" %} double-menu{% endif %}">
	{% if content_only is not defined or not content_only %}
		{% if restricted_country_mode is defined and restricted_country_mode %}
			<section id="restricted-country">
				<p>{l s='You cannot place a new order from your country.'}{% if geolocation_country is defined and geolocation_country %} <span class="bold">{% geolocation_country|escape:'html':'UTF-8' %}</span>{% endif %}</p>
			</section>
		{% endif %}
		<section id="page" data-column="{% colValue %}" data-type="{% LISTING_GRIG_MODE %}">
			<!-- Header -->
			<header id="header">
				<section class="header-container">
					<div id="topbar">
						{capture name='displayBanner'}{hook h='displayBanner'}{% /capture %}
						{% if smarty.capture.displayBanner %}
							<div class="banner">
								<div class="container">
									<div class="row">
										{% smarty.capture.displayBanner %}
									</div>
								</div>
							</div>
						{% endif %}
						{capture name='displayNav'}{hook h='displayNav'}{% /capture %}
						{% if smarty.capture.displayNav %}
						<div class="nav">
																		<div class="container">
								<div class="inner">
									<nav>{% smarty.capture.displayNav %}</nav>
								</div>
							</div>
						</div>
						{% endif %}
					</div>
					<div id="header-main">
						<div class="container">
							<div class="inner">
								<div class="row">
									<div id="header_logo" class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
										<a href="{% if force_ssl %}{% base_dir_ssl %}{% else %}{% base_dir %}{% endif %}" title="{% shop_name|escape:'html':'UTF-8' %}">
												<img class="logo img-responsive" src="{% logo_url %}" alt="{% shop_name|escape:'html':'UTF-8' %}"{% if logo_image_width is defined and logo_image_width %} width="{% logo_image_width %}"{% endif %}{% if logo_image_height is defined and logo_image_height %} height="{% logo_image_height %}"{% endif %}/>
										</a>
									</div>
									<div class="col-xs-12 col-sm-9 col-md-9 col-lg-9 no-padding">
										{% if HOOK_TOP is defined %}{% HOOK_TOP %}{% endif %}
									</div>
								</div>
							</div>
						</div>
					</div>
				</section>
			</header>
			
			{% if HOOK_TOPNAVIGATION is defined and HOOK_TOPNAVIGATION is not empty %}
				<div id="leo-megamenu" class="clearfix">
					<div class="container"> 
					<div class="inner">
							{% HOOK_TOPNAVIGATION %}
					</div>		 
					</div>
				</div>
			{% endif %} 
			
			{% if HOOK_SLIDESHOW is defined and HOOK_SLIDESHOW is not empty and page_name in array('index')|keys %}
				<div id="slideshow" class="clearfix"> 
					<div class="container">
						{% HOOK_SLIDESHOW %}
					</div>
				</div>
			{% endif %}

			<!-- Content -->
			<section id="columns" class="columns-container">
				<div class="container">
					<div class="row">
						{capture name='displayTopColumn'}{hook h='displayTopColumn'}{% /capture %}
						{% if smarty.capture.displayTopColumn %}
						<div id="top_column" class="center_column col-xs-12 col-sm-12 col-md-12">
							{% smarty.capture.displayTopColumn %}
						</div>
						{% endif %}
					</div>
					<div class="row">
                                                
                         
                                                {% if LEO_LAYOUT_DIRECTION is not defined %} {% set LEO_LAYOUT_DIRECTION = "default" %} {% endif %}
						{% include  %}/header.tpl"}  
	{% endif %}
